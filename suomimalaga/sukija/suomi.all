subrule heitto ($tietue):
  $tietue :=+ [lähtösana: $tietue.perusmuoto];
  $tietue.luokka := nimisana;
  $tietue.äs := ääntiösointu ($tietue);
  $tietue.perusmuoto := $tietue.alku + "t"+O($tietue);
  return valo ($tietue, "t"+O($tietue), O($tietue));
